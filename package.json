    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.9.13",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.52",
    "@material-ui/pickers": "^3.2.10",
    "axios": "^0.19.0",
    "chartist": "^0.11.4",
    "classnames": "^2.2.6",
    "date-fns": "^2.11.1",
    "fontello-cli": "^0.5.0",
    "history": "^4.10.1",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-barcode": "^1.4.0",
    "react-chartist": "^0.14.3",
    "react-dom": "^16.11.0",
    "react-highlight-words": "^0.16.0",
    "react-hook-form": "^6.15.2",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-toastify": "^6.1.0",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-multi": "^0.1.12",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "styled-components": "^5.2.1",
    "typeface-roboto": "0.0.75"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "fontello:open": "fontello-cli open --config ./src/assets/vectorIcons/config.json --css ./src/assets/styles/fontello/ --font ./src/assets/fonts/fontello/",
    "fontello:install": "fontello-cli install --config ./src/assets/vectorIcons/config.json --css ./src/assets/fontello/css --font ./src/assets/fontello/font"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "husky": ">=4",
    "lint-staged": ">=10",
    "prettier": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  }
}
